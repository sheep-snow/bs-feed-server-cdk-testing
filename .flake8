[flake8]
ignore = E203, E266, E501, W503, W504
max-line-length = 88
max-complexity = 20
exclude =
    migrations,
    __pycache__,
    manage.py,
    settings.py,
    .env,
    .venv,
    wsgi.py,
    .git,
    docs/source/conf.py,
    alembic/versions/**,
    old,
    build,
    dist,
    .rst,
    .vscode,
    tests/event_datas.py,
    node_modules/